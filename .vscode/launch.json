    "version": "0.2.0",
    "configurations": [
        {
            "name": "eye_care_for_all",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "eye_care_for_all (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "eye_care_for_all (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Flutter (lib\\main.dart)",
            "type": "dart",
            "request": "launch",
            "program": "lib\\main.dart"
        },
        {
            "name": "Flutter (Chrome, disable web security)",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-browser-flag",
                "--disable-web-security"
            ]
        },
        {
            "name": "Flutter (Edge, disable web security)",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "edge",
                "--web-browser-flag",
                "--disable-web-security"
            ]
        }
    ]
}